package com.capitalone.fibonacci;

import java.util.Scanner;

public class FibonacciNos {
	
	public static void main(String[] args) {
		   int n = 0, a = 0, b = 0, c = 1;
	        for(int i = 1; i <= n; i++)
	        {
	            a = b;
	            b = c;
	            c = a + b;
	            System.out.print(a+" ");
	        }

	}

}
