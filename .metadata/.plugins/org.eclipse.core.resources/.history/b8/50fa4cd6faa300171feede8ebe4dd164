package com.capital.one.assigment9.arrayeven;


public class StoreNumberMain {
	int temp;

	public void numberstore(int[] myarr) {
		for(int i =0; i <= myarr.length; i++)
			if((temp & 1) == 0)
			{
				System.out.println("Number Enter " + myarr);
				System.out.println("its even");
			}
			else{
				System.out.println("Number Enter " + myarr);
				System.out.println("its odd");
			}
		}
		
		
		
		
	}
	
	
