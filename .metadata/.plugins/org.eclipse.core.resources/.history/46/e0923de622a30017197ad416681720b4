package com.capitalone.fibonacci;

public class FibonacciNos {

	static int a = 0;
	static int b = 0;
	static int c = 1;
	 static int i;
	 static int count =10;

	public static void main(String[] args) {
		for(int i =2; i <count;++i){
			a = b;
			b = c;
			c = a+b;
			System.out.println(c);
		
			
		}
		
	}

}
