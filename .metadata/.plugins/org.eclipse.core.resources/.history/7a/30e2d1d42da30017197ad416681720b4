package com.capitalone.substring;

public class SubstringAssigment {

	static int n = 0;

	public static void main(String[] args) {

		if (((n / 2) * 2) == n) {
			System.out.println("this number is even "+ n);}
		else {
			System.out.println("this number is odd");
		}

